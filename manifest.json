{
  "display_information": {
    "name": "racoon-radar (local)",
    "background_color": "#32343b"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": false,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "racoon-radar (local)",
      "always_online": true
    },
    "shortcuts": [
      {
        "name": "Suggest for #happenings",
        "type": "message",
        "callback_id": "happenings_submit",
        "description": "let the #happenings team know about a message!"
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": ["channels:history", "chat:write", "im:history", "commands", "users:read", "groups:history", "groups:write","mpim:write","im:write"]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": ["message.channels", "message.im"]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
